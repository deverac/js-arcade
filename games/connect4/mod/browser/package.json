    "clean": "shx rm -rf dist/"
  },
  "dependencies": {
    "@kenrick95/c4": "4.0.0",
    "es6-promise": "^4.2.8",
    "parcel": "^1.12.4",
    "url-search-params-polyfill": "^8.0.0",
    "yarn": "^1.22.5"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.4",
    "shx": "^0.3.2",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "minimist": "^1.2.2"
