{
  "name": "mah",
  "version": "1.1.9",
  "author": "ffalt",
  "license": "MIT",
  "description": "a html5 mahjong solitaire game",
  "repository": "https://github.com/ffalt/mah.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build:deploy-release",
    "build:prebuild": "node ./build-pre.js",
    "build:deploy-release": "npm run build:prebuild && ng build --prod --aot --base-href \".\"",
    "build:deploy-root": "npm run build:prebuild && ng build --prod --aot --base-href \"/\"",
    "build:deploy-ghpages": "npm run build:prebuild && ng build --prod --aot --base-href \"https://ffalt.github.io/mah/\"",
    "deploy": "deplokay -c deplokay-ghpages.json",
    "release": "deplokay -c deplokay-release.json",
    "update:angular": "ng update @angular/cli @angular/core",
    "test": "jest",
    "coverage": "jest --coverage true",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.0",
    "@angular/common": "10.1.0",
    "@angular/compiler": "10.1.0",
    "@angular/core": "10.1.0",
    "@angular/forms": "10.1.0",
    "@angular/platform-browser": "10.1.0",
    "@angular/platform-browser-dynamic": "10.1.0",
    "@angular/platform-server": "10.1.0",
    "@ngx-translate/core": "13.0.0",
    "classlist.js": "1.1.20150312",
    "core-js": "3.6.5",
    "rxjs": "6.6.2",
    "tslib": "2.0.1",
    "tsutils": "3.17.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.0",
    "@angular-devkit/build-angular": "0.1001.0",
    "@angular/cli": "10.1.0",
    "@angular/compiler-cli": "10.1.0",
    "@angular/language-service": "10.1.0",
    "@types/jest": "26.0.13",
    "@types/node": "14.6.4",
    "codelyzer": "6.0.0",
    "deplokay": "0.0.12",
    "is-promise": "4.0.0",
    "jest": "26.4.2",
    "jest-preset-angular": "8.3.1",
    "ng-mocks": "10.1.2",
    "rxjs-tslint": "0.1.8",
    "rxjs-tslint-rules": "4.34.1",
    "tslint": "6.1.3",
    "tslint-etc": "1.13.6",
    "typescript": "3.9.7",
    "webpack": "4.44.1"
  }
}
